{
  "name": "freezy-bee/doctrine-form-mapper",
  "license": [
    "MIT"
  ],
  "type": "library",
  "description": "Forms mapping of Doctrine ORM entities for Nette Framework",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "keywords": [
    "nette",
    "doctrine",
    "forms",
    "mapping"
  ],
  "authors": [
    {
      "name": "Jakub Janata",
      "email": "jakubjanata@gmail.com"
    }
  ],
  "require": {
    "php": "~7.0",
    "nette/di": "~2.4",
    "nette/forms": "~2.4",
    "doctrine/orm": "~2.5",
    "symfony/property-access": "~3.0 || ^4.0"
  },
  "require-dev": {
    "nette/application": "~2.4",
    "nette/bootstrap": "~2.4",
    "latte/latte": "~2.4",
    "nette/tester": "~2.0",
    "squizlabs/php_codesniffer": "^3.0",
    "phpstan/phpstan-shim": "^0.9 || ^0.10"
  },
  "autoload": {
    "psr-4": {
      "FreezyBee\\DoctrineFormMapper\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FreezyBee\\DoctrineFormMapper\\Tests\\": "tests/"
    }
  }
}
